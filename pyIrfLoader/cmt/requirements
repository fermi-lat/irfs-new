use irfLoader v* irfs
use SwigPolicy v*

path_remove PYTHONPATH pyIrfLoader
path_append PYTHONPATH $(PYIRFLOADERROOT)/$(BINDIR)
path_append PYTHONPATH $(PYIRFLOADERROOT)/python

apply_pattern swig_module name="pyIrfLoader"
