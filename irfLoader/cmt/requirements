# $Id$

use STpolicy v1r*
use dc1Response v0* irfs
use dc1aResponse v0* irfs
use g25Response v0* irfs
use testResponse v0* irfs

apply_pattern package_include
apply_pattern package_linkopts
apply_pattern package_stamps

macro_append cppflags "" Linux " -I../src -DTRAP_FPE " 
macro source *.cxx WIN32 "*.h *.cxx"

library irfLoader -no_share $(source)
application test_irfLoader test/*.cxx

private

use cppunit v1r* IExternal
